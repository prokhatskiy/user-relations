{
  "extends": [
    "airbnb",
    "./base.json"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-indent-props": 2,
    "react/jsx-indent": [2, 4]
  }
}
